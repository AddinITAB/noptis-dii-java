			<xs:extension base="PT:PTObjectRef">
				<xs:choice>
					<xs:element name="Number">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="DefiningTransportAuthorityRef" type="PT:TransportAuthorityRef" minOccurs="0"/>
							</xs:sequence>
							<xs:attribute name="Number" type="PT:PlaceNumber" use="required"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="Gid">
						<xs:complexType>
							<xs:attribute name="Gid" type="PT:PlaceGid" use="required"/>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="StopRef">
		<xs:sequence>
			<xs:choice>
				<xs:element name="StopAreaRef" type="PT:StopAreaRef">
					<xs:annotation>
						<xs:documentation>This will always be converted to the set of journey pattern points valid for this stop area at time of import.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="StopPointRef" type="PT:StopPointRef">
					<xs:annotation>
						<xs:documentation>This will always be converted to the journey pattern point valid for this stop point at time of import.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="JourneyPatternPointRef" type="PT:JourneyPatternPointRef"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="StopAreaRef">
		<xs:complexContent>
			<xs:extension base="PT:PTObjectRef">
				<xs:choice>
					<xs:element name="Number">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="DefiningTransportAuthorityRef" type="PT:TransportAuthorityRef" minOccurs="0"/>
							</xs:sequence>
							<xs:attribute name="Number" type="PT:StopAreaNumber" use="required"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="Gid">
						<xs:complexType>
							<xs:attribute name="Gid" type="PT:StopAreaGid" use="required"/>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="JourneyPatternPointRef">
		<xs:complexContent>
			<xs:extension base="PT:PTObjectRef">
				<xs:choice>
					<xs:element name="Number">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="DefiningTransportAuthorityRef" type="PT:TransportAuthorityRef" minOccurs="0"/>
							</xs:sequence>
							<xs:attribute name="Number" type="PT:JourneyPatternPointNumber" use="required"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="Gid">
						<xs:complexType>
							<xs:attribute name="Gid" type="PT:JourneyPatternPointGid" use="required"/>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="StopPointRef">
		<xs:complexContent>
			<xs:extension base="PT:PTObjectRef">
				<xs:choice>
					<xs:element name="LocalNumber">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="StopAreaRef" type="PT:StopAreaRef"/>
							</xs:sequence>
							<xs:attribute name="LocalNumber" type="PT:StopPointNumber" use="required">
								<xs:annotation>
									<xs:documentation>Local StopPoint Number within the StopArea.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
					<xs:element name="Gid">
						<xs:annotation>
							<xs:documentation>Either StopPointGid or JourneyPatternPointGid</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:attribute name="Gid" type="PT:StopPointGid" use="required"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="JourneyPatternPointNumber">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="DefiningTransportAuthorityRef" type="PT:TransportAuthorityRef" minOccurs="0"/>
							</xs:sequence>
							<xs:attribute name="Number" type="PT:JourneyPatternPointNumber" use="required"/>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="User">
		<xs:annotation>
			<xs:documentation>Characteristics of a user.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Name" type="PT:Name" use="required"/>
		<xs:attribute name="EMail" type="PT:EMail" use="optional"/>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="Application">
		<xs:annotation>
			<xs:documentation>Characteristics of a computer software.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Name" type="PT:Name" use="required"/>
		<xs:attribute name="VersionComment" type="PT:Comment" use="optional"/>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="CommunicationAddress">
		<xs:complexContent>
			<xs:extension base="PT:ExtendablePTObject">
				<xs:sequence>
					<xs:element name="ValidDatePeriod" type="PT:PeriodOfDays" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="Name" type="PT:Name" use="required">
					<xs:annotation>
						<xs:documentation>For example a phonenumber "123456".</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Protocol" type="PT:CommunicationProtocol" use="required">
					<xs:annotation>
						<xs:documentation>For example "PHONE".</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:simpleType name="CommunicationProtocol">
		<xs:annotation>
			<xs:documentation>I. e. PHONE, FAX, TETRA</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="PHONE"/>
			<xs:enumeration value="FAX"/>
			<xs:enumeration value="SMS"/>
			<xs:enumeration value="EMAIL"/>
			<xs:enumeration value="PAGER"/>
			<xs:enumeration value="TETRA"/>
			<xs:enumeration value="MOBITEX"/>
			<xs:enumeration value="MRS5000"/>
			<xs:enumeration value="UNKNOWN"/>
		</xs:restriction>
	</xs:simpleType>
	<!--  *********************************************************  -->
	<xs:complexType name="Origin">
		<xs:choice>
			<xs:element name="PlaceRef" type="PT:PlaceRef"/>
			<xs:element name="Text">
				<xs:complexType>
					<xs:attribute name="Name" type="PT:Name" use="required"/>
					<xs:attribute name="ShortName" type="PT:ShortName" use="optional">
						<xs:annotation>
							<xs:documentation>The short form of the primary name, used when there is limited space to display the name.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="Destination">
		<xs:choice>
			<xs:element name="PlaceRef" type="PT:PlaceRef"/>
			<xs:element name="Text">
				<xs:complexType>
					<xs:attribute name="Name" type="PT:Name" use="required"/>
					<xs:attribute name="ShortName" type="PT:ShortName" use="optional">
						<xs:annotation>
							<xs:documentation>The short form of the primary name, used when there is limited space to display the name.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="DestinationDisplay">
		<xs:sequence>
			<xs:element name="Keys" type="PT:Keys" minOccurs="0"/>
			<xs:element name="PrimaryDestination" type="PT:Destination">
				<xs:annotation>
					<xs:documentation>The primary destination, usually the name of end area, city or terminal of the VehicleJourney. It may or may not be the name of the last StopArea.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SecondaryDestination" minOccurs="0">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="PT:Destination">
							<xs:attribute name="MeaningOfSecondaryDestination" type="PT:SecondaryDestinationType" use="required"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="ExternalKeys" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Activation codes used by  different devices such as headsigns and announcement systems to display or announce destination from this stop and on. Can also be used for last stop messages and overriding next stop messages concerning this stop.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Key" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="ParameterData" type="PT:ParameterData" use="required">
									<xs:annotation>
										<xs:documentation>The codes that the driver should key in or that should be sent to the display system if it not is capable of handling free text format.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="DeviceName" type="PT:Name" use="required">
									<xs:annotation>
										<xs:documentation>The name of the device/system where the code is relevant.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Type" type="PT:DisplayKeyType" use="optional" default="DESTINATION"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ProductName" type="PT:Name" use="optional">
			<xs:annotation>
				<xs:documentation>Marketed name of the product.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LineDesignation" type="PT:LineDesignation" use="optional">
			<xs:annotation>
				<xs:documentation>The designation (usually the Line Number) as it is displayed to the public.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SymbolName" type="PT:Name" use="optional"/>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="ServiceRequirement">
		<xs:sequence>
			<xs:element name="Keys" type="PT:Keys" minOccurs="0"/>
			<xs:element name="Capacity" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="MinimumSeatingsCount" type="PT:Count" use="optional"/>
					<xs:attribute name="MinimumTotalCount" type="PT:Count" use="optional"/>
					<xs:attribute name="MinimumWheelChairsCount" type="PT:Count" use="optional"/>
					<xs:attribute name="MinimumPramsCount" type="PT:Count" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="TrainSize" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="CarCount" type="PT:Count" use="optional"/>
					<xs:attribute name="TrainLengthType" type="PT:TrainLengthType" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="BusSize" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="SizeType" type="PT:BusSizeType" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Engine" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="RequiredEmissionLevel" type="PT:EmissionLevel" use="optional"/>
					<xs:attribute name="RequiredFuelType" type="PT:FuelType" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Accessibility" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="IsLowEntrance" type="PT:YesNo" use="optional" default="N"/>
					<xs:attribute name="HasLiftOrRamp" type="PT:YesNo" use="optional" default="N"/>
					<xs:attribute name="IsLowFloor" type="PT:YesNo" use="optional" default="N"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="TransportMode" type="PT:TransportMode" use="optional"/>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="Arrival">
		<xs:attribute name="TypeOfArrival" type="PT:ArrivalType" use="optional" default="STOP_IF_ALIGHTING">
			<xs:annotation>
				<xs:documentation>Indicates conditions for arrivals at the Call.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DayOffset" type="PT:DayOffset" use="optional" default="0">
			<xs:annotation>
				<xs:documentation>The day number of the latest arrival time, counted from the defined operating day. The value will be 0 if the arrival takes place on the same date as the operating day of the journey. If the arrival is after midnight, and the journey starts before midnight, the value will be 1. If this attribute is left out, it is assumed that a value of 0 is implied. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LatestTime" type="PT:TimeOffset" use="required">
			<xs:annotation>
				<xs:documentation>The latest arrival time accepted without regarding it as delayed.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="Departure">
		<xs:attribute name="TypeOfDeparture" type="PT:DepartureType" use="optional" default="STOP_IF_BOARDING">
			<xs:annotation>
				<xs:documentation>Indicates conditions for departures at the Call.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DayOffset" type="PT:DayOffset" use="optional" default="0">
			<xs:annotation>
				<xs:documentation>The day number of the  earliest departure time, counted from the defined operating day. The value will be 0 if the departure takes place on the same date as the  operating day of the journey. If the departure is after midnight, and the journey starts before midnight, the value will be 1.  If this attribute is left out, it is assumed that a value of 0 is implied. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="EarliestTime" type="PT:TimeOffset" use="required">
			<xs:annotation>
				<xs:documentation>The earliest permitted departure time.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="SimpleBasicCall">
		<xs:annotation>
			<xs:documentation>A call is the combined actions related to a bus starting, ending, standing or passing a journey pattern point. It will typically contain information about the arrival time and/or the departure time. For service journeys the first and last call must be to a stop point. For dead runs the first and last call must be either to a stop point or a parking point. Intermediate calls can also be made to via points.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PT:PTObject">
				<xs:choice>
					<xs:element name="StopPointRef" type="PT:StopPointRef">
						<xs:annotation>
							<xs:documentation>The importing application will convert StopPointRef to the JourneyPatternPointRef that is associated at the time of import.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="JourneyPatternPointRef" type="PT:JourneyPatternPointRef"/>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="BasicCall">
		<xs:annotation>
			<xs:documentation>A call is the combined actions related to a bus starting, ending, standing or passing a journey pattern point. It will typically contain information about the arrival time and/or the departure time. For service journeys the first and last call must be to a stop point. For dead runs the first and last call must be either to a stop point or a parking point. Intermediate calls can also be made to via points.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PT:ExtendablePTObject">
				<xs:choice>
					<xs:element name="StopPointRef" type="PT:StopPointRef">
						<xs:annotation>
							<xs:documentation>The importing application will convert StopPointRef to the JourneyPatternPointRef that is associated at the time of import.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="JourneyPatternPointRef" type="PT:JourneyPatternPointRef"/>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="BasicSequencedCall">
		<xs:complexContent>
			<xs:extension base="PT:BasicCall">
				<xs:attribute name="SequenceNumber" type="PT:SequenceNumber" use="required">
					<xs:annotation>
						<xs:documentation>The sequence number must be 1 for the first call. The following calls must be provided in sequence and have sequence numbers that are increasing without gaps.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="BasicServiceJourneyCall">
		<xs:annotation>
			<xs:documentation>The Call-record contains information about one passage at a stop without any timing information.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PT:BasicSequencedCall">
				<xs:sequence>
					<xs:element name="NewDestinationDisplay" type="PT:DestinationDisplay" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Destination as it should be shown from this point and on and any specific display information concerning this stop.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NewServiceRequirement" type="PT:ServiceRequirement" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Service requirement from this point and on.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="BasicFirstCall">
		<xs:complexContent>
			<xs:extension base="PT:BasicCall">
				<xs:sequence>
					<xs:element name="Departure">
						<xs:complexType>
							<xs:attribute name="DayOffset" type="PT:DayOffset" use="optional" default="0">
								<xs:annotation>
									<xs:documentation>The day number of the  earliest departure time, counted from the defined operating day. The value will be 0 if the departure takes place on the same date as the  operating day of the journey. If the departure is after midnight, and the journey starts before midnight, the value will be 1.  If this attribute is left out, it is assumed that a value of 0 is implied. </xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="EarliestTime" type="PT:TimeOffset" use="required">
								<xs:annotation>
									<xs:documentation>The earliest permitted departure time.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="BasicLastCall">
		<xs:complexContent>
			<xs:extension base="PT:BasicCall">
				<xs:sequence>
					<xs:element name="Arrival">
						<xs:complexType>
							<xs:attribute name="DayOffset" type="PT:DayOffset" use="optional" default="0">
								<xs:annotation>
									<xs:documentation>The day number of the latest arrival time, counted from the defined operating day. The value will be 0 if the arrival takes place on the same date as the operating day of the journey. If the arrival is after midnight, and the journey starts before midnight, the value will be 1.  If this attribute is left out, it is assumed that a value of 0 is implied. </xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="LatestTime" type="PT:TimeOffset" use="required"/>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="Location">
		<xs:annotation>
			<xs:documentation>A 2D coordinate. (3D optional)</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Northing" type="PT:Northing" use="required">
			<xs:annotation>
				<xs:documentation>The north/south co-ordinate value. The interpretation is dependent by the used Coordinate System.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Easting" type="PT:Easting" use="required">
			<xs:annotation>
				<xs:documentation>The east/west co-ordinate value. The interpretation is dependent by the used Coordinate System.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CoordinateSystem" type="PT:Name" use="optional">
			<xs:annotation>
				<xs:documentation>An identification of the co-ordinate system used for the definition of the location of Mapping Points. Defaults to Topology.DefaultCoordinateSystem.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="Notes">
		<xs:sequence>
			<xs:element name="Note" type="PT:DetailedNote" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="DetailedNote">
		<xs:sequence>
			<xs:element name="Keys" type="PT:Keys" minOccurs="0"/>
			<xs:element name="ExternalRef" type="PT:ExternalNoteRef" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identification of this note in an external system.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Text" type="PT:Note" use="required"/>
		<xs:attribute name="ConcernsArrivals" type="PT:YesNo" use="optional" default="Y"/>
		<xs:attribute name="ConcernsDepartures" type="PT:YesNo" use="optional" default="Y"/>
		<xs:attribute name="IsExposedToStaff" type="PT:YesNo" use="optional" default="Y">
			<xs:annotation>
				<xs:documentation>Yes means that the information is intended for drivers and other staff.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="IsPublic" type="PT:YesNo" use="optional" default="Y">
			<xs:annotation>
				<xs:documentation>Yes means that this information is intended for passenger information.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="IsExposedInPrintMedia" type="PT:YesNo" use="optional" default="Y"/>
		<xs:attribute name="IsExposedInDynamicMedia" type="PT:YesNo" use="optional" default="Y"/>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="ExternalNoteRef">
		<xs:attribute name="NoteID" type="PT:Name" use="required">
			<xs:annotation>
				<xs:documentation>This is a unique note identification in scope of a certain external source system.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SystemID" type="PT:Name" use="required">
			<xs:annotation>
				<xs:documentation>This is the agreed identification of the external source system.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:simpleType name="DayTypeCode">
		<xs:annotation>
			<xs:documentation>Code for daytype. Could be a PublicDayType or a SpecialDayType.</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="PT:PublicDayTypeCode PT:SpecialDayTypeCode"/>
	</xs:simpleType>
	<xs:simpleType name="PublicDayTypeCode">
		<xs:annotation>
			<xs:documentation>A Day Type Code that starts with a digit.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:maxLength value="8"/>
			<xs:pattern value="\d{1}.*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ServiceDayTypeCode">
		<xs:annotation>
			<xs:documentation>A Day Type Code that starts with a digit, but is not the single digits 1 through 7. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:maxLength value="8"/>
			<xs:pattern value="[0,8,9].*|[1,2,3,4,5,6,7].+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SpecialDayTypeCode">
		<xs:annotation>
			<xs:documentation>A Day Type Code that starts with a letter. Observe that up to 16 characters is permitted.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:maxLength value="16"/>
			<xs:pattern value="[A-Z,a-z].*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Abbreviation">
		<xs:annotation>
			<xs:documentation>User defined.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Initials">
		<xs:annotation>
			<xs:documentation>User defined.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="8"/>
			<xs:pattern value="\p{Lu}*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ModificationType">
		<xs:annotation>
			<xs:documentation>General - A versiondocument can never affect objects that reside in another versionframe.
			A versiondocument can never affect objects that reside outside the given timeframe. If objects exist both outside and inside the given timeframe, then parts outside the timeframe will remain, while those within will be affected.
		
			DEFINE - All objects not mentioned in this version document will be closed, those mentioned will be added or modified.
			MODIFY - Objects mentioned in this version document will be added or modified.
			DELETE - All objects mentioned will be closed.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="MODIFY"/>
			<xs:enumeration value="DELETE"/>
			<xs:enumeration value="DEFINE"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BusSizeType">
		<xs:annotation>
			<xs:documentation>Code for generic bus sizes.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="ARTICUL">
				<xs:annotation>
					<xs:documentation>Articulated bus (ledbuss)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BOGGIE">
				<xs:annotation>
					<xs:documentation>Boggie bus</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DDECKER">
				<xs:annotation>
					<xs:documentation>Double decker bus</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MINI">
				<xs:annotation>
					<xs:documentation>Mini bus</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SMALL">
				<xs:annotation>
					<xs:documentation>Small bus</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="STANDARD">
				<xs:annotation>
					<xs:documentation>Standard or normal bus</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Code">
		<xs:annotation>
			<xs:documentation>A user defined value, often used as an identifier.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Comment">
		<xs:annotation>
			<xs:documentation>An internal information not released to the public.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Count">
		<xs:annotation>
			<xs:documentation>A non-negative integer.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:unsignedInt"/>
	</xs:simpleType>
	<xs:simpleType name="SequenceNumber">
		<xs:annotation>
			<xs:documentation>The position in an ordered list of some kind. Synonym with ORDER according to Transmodel</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="10000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PosteriorSequenceNumber">
		<xs:annotation>
			<xs:documentation>The position in an ordered list of some kind, excluding the first position.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:minInclusive value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ContractorNumber">
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="9999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ParameterData">
		<xs:annotation>
			<xs:documentation>Block of information whose structure is defined in external proprietary systems.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Date">
		<xs:annotation>
			<xs:documentation>An absolute date defined according to xs:date format. CCYY-MM-DD</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:date"/>
	</xs:simpleType>
	<xs:simpleType name="DateTime">
		<xs:annotation>
			<xs:documentation>Date and time  defined according to xs:dateTime format. CCYY-MM-DDThh:mm:ss.sss</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="DayOffset">
		<xs:annotation>
			<xs:documentation>Used in combination with TimeOffset to express timespans larger than 24 hours</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:nonNegativeInteger">
			<xs:maxInclusive value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Duration">
		<xs:annotation>
			<xs:documentation>A timespan</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:duration"/>
	</xs:simpleType>
	<xs:simpleType name="EMail">
		<xs:annotation>
			<xs:documentation>EMail-address.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:pattern value=".+@.+\..+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="EmissionLevel">
		<xs:annotation>
			<xs:documentation>Emission standard according to European norms</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="EURO_I"/>
			<xs:enumeration value="EURO_II"/>
			<xs:enumeration value="EURO_III"/>
			<xs:enumeration value="EURO_IV"/>
			<xs:enumeration value="EURO_V"/>
			<xs:enumeration value="EURO_VI"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ExternalID">
		<xs:annotation>
			<xs:documentation>External identification of this object.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="25"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ExternalSourceReference">
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FuelType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="BATTERY">
				<xs:annotation>
					<xs:documentation>Electric (battery)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BIOGAS">
				<xs:annotation>
					<xs:documentation>Biogas</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CNG">
				<xs:annotation>
					<xs:documentation>Natural gas.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DIESEL">
				<xs:annotation>
					<xs:documentation>Diesel</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DIESELN">
				<xs:annotation>
					<xs:documentation>Diesel, standard</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DIESELL">
				<xs:annotation>
					<xs:documentation>Diesel, light</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E85">
				<xs:annotation>
					<xs:documentation>Blend of 85% ethanol and 15% petrol</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ELECTRIC">
				<xs:annotation>
					<xs:documentation>Electric (overhead wire or equivalent)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HYBRIDDB">
				<xs:annotation>
					<xs:documentation>Hybrid, diesel/battery</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HYDROGEN">
				<xs:annotation>
					<xs:documentation>Hydrogen</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LPG">
				<xs:annotation>
					<xs:documentation>LPG.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PETROLL">
				<xs:annotation>
					<xs:documentation>Petrol, leaded</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PETROLUL">
				<xs:annotation>
					<xs:documentation>Petrol, unleaded</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UNKNOWN">
				<xs:annotation>
					<xs:documentation>Fuel unknown</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="GenericDirection">
		<xs:annotation>
			<xs:documentation>Used to separate routes (journey patterns) into two main groups running in opposite directions. The grouping could be based on such aspects as the direction being inward, outward or clockwise, anti-clockwise. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="ODD"/>
			<xs:enumeration value="EVEN"/>
		</xs:restriction>
	</xs:simpleType>
	<!--  *********************************************************  -->
	<!--  *********************************************************  -->
	<xs:simpleType name="PointGid">
		<xs:restriction base="xs:unsignedLong">
			<xs:minInclusive value="9022000000000000"/>
			<xs:maxInclusive value="9025999999999999"/>
		</xs:restriction>
	</xs:simpleType>
	<!--  -->
	<xs:simpleType name="TransportAuthorityGid">
