	<!--  *********************************************************  -->
	<xs:complexType name="Tasks">
		<xs:annotation>
			<xs:documentation>Tasks can be either ServiceJourneys, Dead Runs or external tasks. External tasks indicates that a logical vehicle is occupied with something else than a defined ServiceJourney or DeadRun. For simplicity external tasks should be represented as Dead Runs</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="ServiceJourneyRef" type="PT:ExtendableServiceJourneyRef"/>
			<xs:element name="ServiceJourneyDef" type="PT:ExtendableServiceJourneyDef"/>
			<xs:element name="DeadRunRef" type="PT:ExtendableDeadRunRef"/>
			<xs:element name="DeadRunDef" type="PT:ExtendableDeadRunDef"/>
		</xs:choice>
	</xs:complexType>
