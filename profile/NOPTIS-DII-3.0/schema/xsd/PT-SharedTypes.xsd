							<xs:documentation>This is the number assigned to a garage in a garage utilisation by a defining contractor.  </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="DefiningContractorRef" type="PT:ContractorRef" minOccurs="0"/>
							</xs:sequence>
							<xs:attribute name="Number" type="PT:GarageNumber" use="required"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="GarageName">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="OwningOrganisationRef" type="PT:OrganisationRef" minOccurs="0"/>
							</xs:sequence>
							<xs:attribute name="Name" type="PT:Name" use="required"/>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:extension>
