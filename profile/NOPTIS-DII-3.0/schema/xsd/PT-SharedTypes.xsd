				<xs:sequence>
					<xs:element name="FirstCall" type="PT:BasicFirstCall"/>
					<xs:element name="LastCall" type="PT:BasicLastCall"/>
					<xs:element name="DirectionOfLineRef" type="PT:DirectionOfLineRef"/>
