	<xs:simpleType name="JourneyNumber">
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="99999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="JourneyPatternPointNumber">
		<xs:annotation>
			<xs:documentation>A unique number within a transport authority used for StopPoints, ViaPoints and ParkingPoints.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="99999999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LineDesignation">
		<xs:annotation>
			<xs:documentation>The public identification of the Line, used in all passenger information.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LineNumber">
		<xs:annotation>
			<xs:documentation>Technical number for a Line. This number must be unique in the scope of a transport authority any given date. See also LineDesignation.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="9999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Meters">
		<xs:annotation>
			<xs:documentation>A distance or length in meters.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0.00"/>
			<xs:maxInclusive value="9999999999.99"/>
			<xs:totalDigits value="12"/>
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Name">
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Percent">
		<xs:annotation>
			<xs:documentation> 0 - 100</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:nonNegativeInteger">
			<xs:maxInclusive value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PersonRef">
		<xs:annotation>
			<xs:documentation> A reference to a person. For example social security number.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SecondaryDestinationType">
		<xs:annotation>
			<xs:documentation>Denotes how the second "row" of the destination text should be interpreted. I. e.  a via destination </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="ENDS_AT">
				<xs:annotation>
					<xs:documentation>Denotes that the Secondary Destination is a more precise identification of the Primary Destination, like a terminal name when the Primary Destination is a city.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VIA">
				<xs:annotation>
					<xs:documentation>Denotes that the Secondary Destination is a via-name.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TRANSFER_AT">
				<xs:annotation>
					<xs:documentation>Transfer at secondary destination and continue to primary destination.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CONTINUE_TO">
				<xs:annotation>
					<xs:documentation>Continue to secondary destination after transferring at primary destination.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MESSAGE">
				<xs:annotation>
					<xs:documentation>Denotes that the Secondary Destination is a message of some kind, like when announcing the VehicleJourney as an extra run.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UNKNOWN">
				<xs:annotation>
					<xs:documentation>Denotes that the Secondary Destination is undefined.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="HyperLinkName">
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="1024"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ShortName">
		<xs:annotation>
			<xs:documentation>.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="16"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StopPointNumber">
		<xs:annotation>
			<xs:documentation>The local number within the StopArea</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StopAreaNumber">
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="999999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Time">
		<xs:complexContent>
			<xs:extension base="PT:PTObjectRef">
				<xs:attribute name="TimeOffset" type="PT:TimeOffset" use="required"/>
				<xs:attribute name="DayOffset" type="PT:DayOffset" use="optional" default="0"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:simpleType name="TimeOffset">
		<xs:annotation>
			<xs:documentation>Time from midnight expressed as hh:mm:ss.sss</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:time"/>
	</xs:simpleType>
	<xs:simpleType name="TransportAuthorityNumber">
		<xs:annotation>
			<xs:documentation>Number of the transport authority. The number is provided by PubTrans User group for scandinavian implemetations.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TransportMode">
		<xs:annotation>
			<xs:documentation>Classification of transport systems. I. e. TRAIN, FERRY</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="BUS"/>
			<xs:enumeration value="TRAM"/>
			<xs:enumeration value="METRO"/>
			<xs:enumeration value="TRAIN"/>
			<xs:enumeration value="FERRY"/>
			<xs:enumeration value="SHIP"/>
			<xs:enumeration value="TAXI"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PlaceNumber">
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="99999999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="YesNo">
		<xs:annotation>
			<xs:documentation>Attributes with names starting with Has... Is... or Requires...  uses this simple type. Y for Yes and N for No.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="Y"/>
			<xs:enumeration value="N"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="GarageNumber">
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Note">
		<xs:annotation>
			<xs:documentation>A supplementary information.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CountryCode">
		<xs:annotation>
			<xs:documentation>ISO 3166-1-alpha-2 Country Codes. For example SE -Sweden, DK- Denmark and NO-Norway</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LanguageCode">
		<xs:annotation>
			<xs:documentation>ISO 639.1 Language Codes. For example sv -swedish,  da - danish, en -english, nn - nynorsk norwegian and nb -bokmål norwegian</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:language"/>
	</xs:simpleType>
	<xs:simpleType name="ArrivalType">
		<xs:annotation>
			<xs:documentation>Denotes conditions for alighting at a stop or flexible alighting up to this point.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="NO_STOP"/>
			<xs:enumeration value="NO_ALIGHTING"/>
			<xs:enumeration value="STOP_IF_ALIGHTING"/>
			<xs:enumeration value="ALWAYS_STOP"/>
			<xs:enumeration value="FLEXIBLE_ALIGHTING_UPTO_HERE"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DepartureType">
		<xs:annotation>
			<xs:documentation>Denotes conditions for boarding at a stop or flexible boarding from this point.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="NO_STOP"/>
			<xs:enumeration value="NO_BOARDING"/>
			<xs:enumeration value="STOP_IF_BOARDING"/>
			<xs:enumeration value="ALWAYS_STOP"/>
			<xs:enumeration value="FLEXIBLE_BOARDING_FROM_HERE"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DisplayKeyType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="DESTINATION">
				<xs:annotation>
					<xs:documentation>Code used when key corresponds to a certain destination. I.e. "Slussen" or "Linje 112; Slussen via Gärdet".</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OVERRIDING_STOPNAME">
				<xs:annotation>
					<xs:documentation>Code used when key corresponds to the name of the stop. I.e "Ekeby"</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OVERRIDING_STOPMESSAGE">
				<xs:annotation>
					<xs:documentation>Code for data containing a message that should be announced or displayed instead of the normal STOPMESSAGE for this stop.  I.e  "Next stop Ekeby, thereafter no stop before Slussen"</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ADDITIONAL_INFO">
				<xs:annotation>
					<xs:documentation>Code used when key corresponds to additional information concerning this stop. I.e  "Mind your step when disembarking" or "Last stop, all passengers must disembark" </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OVERRIDING_CHANGE_INFO">
				<xs:annotation>
					<xs:documentation>Code used when key corresponds to additional information concerning interchange at this stop. I.e  "Change to subway"</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TrainLengthType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="SHORT">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LONG">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Coordinate">
		<xs:annotation>
			<xs:documentation>One dimension in a co-ordinate.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:minLength value="1"/>
			<xs:maxLength value="16"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Northing">
		<xs:annotation>
			<xs:documentation>One dimension in a co-ordinate.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="PT:Coordinate"/>
	</xs:simpleType>
	<xs:simpleType name="Easting">
		<xs:annotation>
			<xs:documentation>One dimension in a co-ordinate.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="PT:Coordinate"/>
	</xs:simpleType>
</xs:schema>
