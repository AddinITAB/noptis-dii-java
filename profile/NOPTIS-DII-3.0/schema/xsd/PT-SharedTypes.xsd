			<xs:documentation>---.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ValidFromDate" type="PT:Date" use="required"/>
		<xs:attribute name="InvalidFromDate" type="PT:Date" use="optional"/>
	</xs:complexType>
	<!--  *********************************************************  -->
	<xs:complexType name="FixedPeriodOfDays">
		<xs:annotation>
			<xs:documentation>---.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ValidFromDate" type="PT:Date" use="required"/>
		<xs:attribute name="InvalidFromDate" type="PT:Date" use="required"/>
